{
  "name": "@nsilly/support",
  "version": "2.0.0",
  "description": "NSilly Support package",
  "main": "dist/src/index.js",
  "scripts": {
    "test": "jest --forceExit",
    "build": "rimraf dist && ./node_modules/.bin/babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log,error_code.md,README.md --copy-files",
